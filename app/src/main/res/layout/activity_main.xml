<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MainActivity"
    android:id="@+id/init_frame"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/init_frame"
    >

    <RelativeLayout
        android:id="@+id/logo"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/logo"
        android:clipToOutline="true" />

    <TextView
        android:id="@+id/we_remember"
        android:layout_width="296dp"
        android:layout_height="37dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="32dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="350dp"
        android:text="We Remember For You"
        android:textAppearance="@style/we_remember"
        android:gravity="center_horizontal|top"
        />

    <Button
        android:id="@+id/login_butto"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="499dp"
        android:background="@drawable/login_butto"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:visibility="visible"
        tools:text="Login" />

    <Button
        android:id="@+id/signup_butt"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="597dp"
        android:background="@drawable/signup_butt"
        android:clipToOutline="true"
        android:elevation="4dp"
        android:visibility="visible"
        tools:text="SignUp" />


</RelativeLayout>


